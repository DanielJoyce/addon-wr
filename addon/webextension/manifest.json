{
  "name": "WebExtension-whiterose",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
    "content_scripts": [
   {
     "matches": ["<all_urls>"],
     "js": ["vendor/findAndReplaceDOMText.js", "content-script.js"]
   }
 ]
}
